{
    "name": "Jenkins",
    "viewName": "Jenkins",
    "type": "executer",
    "execProgram": "node",
    "main": "app.js",
    "version": "2.2.0",
    "imgUrl": "logo.png",
    "description": "Automation for continuous integration and continuous delivery",
    "category": "CI/CD",
    "keywords": [
      "jenkins",
      "build",
      "job"
    ],
    "auth": {
      "authId": "jenkinsAccount",
      "params": [
        {
          "name": "URL",
          "viewName": "Jenkins URL",
          "type": "string"
        },
        {
          "name": "user",
          "viewName": "Username",
          "type": "string"
        },
        {
          "name": "TOKEN",
          "viewName": "Token",
          "type": "vault"
        }
      ]
    },
    "settings": [],
    "methods": [
      {
        "name": "buildJob",
        "viewName": "Build Job",
        "params": [
          {
            "name": "JOB",
            "viewName": "Job name",
            "placeholder": "myJobName",
            "type": "string"
          },
          {
            "name": "PARAMETERS",
            "viewName": "Parameters",
            "description": "Parameters as key=value pairs, one per line, or if using code layer a JSON object",
            "placeholder": "BRANCH=dev\nMODE=debug",
            "type": "text",
            "parserType": "keyValuePairs",
            "learnUrl": "https://www.jenkins.io/doc/pipeline/steps/pipeline-build-step/#build-build-a-job" 
          },
          {
            "name": "waitForEnd",
            "viewName": "Wait for build end",
            "description": "Determine if the action should wait for the build to finish or not.",
            "type": "boolean"
          },
          {
            "name": "failOnFailure",
            "viewName": "Fail on build failure",
            "description": "Determine if the action should fail in case the Jenkins build fails.",
            "type": "boolean"
          }
        ]
      }
    ]
}
