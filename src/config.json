{
    "name": "Jenkins",
    "viewName": "Jenkins",
    "type": "executer",
    "execProgram": "node",
    "main": "app.js",
    "version": "2.0.3",
    "imgUrl": "jenkins.png",
    "description": "Integration with Jenkins.",
    "category": "CI/CD",
    "keywords": [
        "jenkins",
        "build",
        "job"
        ],
    "settings": [
      {
        "name": "URL",
        "viewName": "Jenkins Url",
        "type": "string"
      },
      {
        "name": "user",
        "viewName": "Username",
        "type": "string"
      },
      {
        "name": "TOKEN",
        "viewName": "Token",
        "type": "vault"
      },
      {
        "name": "crumbIssuer",
        "viewName": "CSRF Enabled",
        "type": "boolean"
      }
    ],
    "methods": [
      {
        "name": "buildJob",
        "viewName": "Build job",
        "params": [
          {
            "name": "JOB",
            "viewName": "Job name",
            "placeholder": "myJobName",
            "type": "string"
          },
          {
            "name": "PARAMETERS",
            "description": "Pass a key-value code object of parameters. For example {param1: 'paramValue'}",
            "placeholder": "{param1: 'paramValue'}",
            "viewName": "Parameters (Object)",
            "type": "string",
            "learnUrl": "https://www.jenkins.io/doc/pipeline/steps/pipeline-build-step/#build-build-a-job" 
          },
          {
            "name": "waitForEnd",
            "viewName": "Wait for build end",
            "description": "Determine if the action should wait for the build to finish or not.",
            "type": "boolean"
          },
          {
            "name": "failOnFailure",
            "viewName": "Fail on build failure",
            "description": "Determine if the action should fail in case the Jenkins build fails.",
            "type": "boolean"
          }
        ]
      }
    ]
}
